{
    "configCollectors": {
        "nginxCollector": {
            "logPath": "/var/log/nginx/nginx_metrics.log",
            "url": "http://localhost/nginx_status",
            "scrapeIntervalsec": "15",
            "logFormat": "$remote_addr - $remote_user [$time_local] \"$request\" $status $body_bytes_sent \"$http_referer\" \"$http_user_agent\""
        }
    }
}
